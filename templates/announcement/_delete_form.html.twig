<form class="d-flex justify-content-start" method="post" action="{{ path('announcement_delete', {'id': announcement.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ announcement.id) }}">
    <i class="far fa-trash-alt"></i><button class="card-link text-dark">Delete</button>
</form>